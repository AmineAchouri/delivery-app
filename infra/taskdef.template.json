{
    "containerDefinitions": [
        {
            "cpu": 256,
            "environment": [
                { "name": "PORT", "value": "3000" },
                { "name": "FEATURE_KEY", "value": "core" },
                { "name": "JWT_PRIVATE_KEY", "value": "/delivery/test/JWT_PRIVATE_KEY" },
                { "name": "TOKEN_ISS", "value": "delivery-app" },
                { "name": "DEPLOY_ENV", "value": "test" },
                { "name": "JWT_PUBLIC_KEY", "value": "/delivery/test/JWT_PUBLIC_KEY" },
                { "name": "TOKEN_AUD_TENANT", "value": "tenant-api" },
                { "name": "FEATURE_FLAGS", "value": "" }
            ],
            "essential": true,
                "image": "${IMAGE_URI}",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/ecs/delivery-test-cluster/delivery-core-api-0541-57b6",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memoryReservation": 1024,
            "mountPoints": [],
            "name": "Main",
            "portMappings": [
                { "containerPort": 3000, "hostPort": 3000, "protocol": "tcp" }
            ],
            "secrets": [
                { "name": "DATABASE_URL", "valueFrom": "/delivery/test/DATABASE_URL" }
            ],
            "volumesFrom": []
        }
    ],
    "cpu": "256",
    "enableFaultInjection": false,
        "executionRoleArn": "${EXEC_ROLE_ARN}",
    "family": "${TASKDEF_FAMILY}",
    "memory": "1024",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
        "taskRoleArn": "${TASK_ROLE_ARN}",
    "volumes": []
}